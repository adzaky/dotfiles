{
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "C_Cpp.configurationWarnings": "Disabled",
  "C_Cpp.default.cppStandard": "c++14",
  "C_Cpp.default.cStandard": "c99",
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "C_Cpp.dimInactiveRegions": false,
  "C_Cpp.errorSquiggles": "Disabled",
  "C_Cpp.loggingLevel": "None",
  "C_Cpp.updateChannel": "Insiders",
  "C_Cpp.workspaceParsingPriority": "medium",
  "dart.allowAnalytics": false,
  "dart.devToolsBrowser": "default",
  "dart.hotReloadProgress": "statusBar",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontFamily": "'JetBrainsMono NF Medium', 'JetBrainsMono Nerd Font Medium', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.terminalAuthentication": false,
  "github.gitAuthentication": false,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.gitCommands.closeOnFocusOut": true,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.integratedConsole.suppressStartupBanner": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'JetBrainsMono NF', 'JetBrainsMono Nerd Font Mono', monospace",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": "bold",
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "args": [
        "-NoLogo"
      ]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "update.mode": "manual",
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Cherry",
  "workbench.tree.indent": 10,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  }
}
