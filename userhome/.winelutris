LUTRIS_WINEFOLDERS=$HOME/.local/share/lutris/runners/wine
LUTRIS_WINEVER=$(find $LUTRIS_WINEFOLDERS -type f -name wine64 | sed 's|.*nners/wine/\(.*\)/bin/wi.*|\1|')

if [ "$LUTRIS_WINE_SOURCED" != 1 ] && [ -f $LUTRIS_WINEFOLDERS/$LUTRIS_WINEVER/bin/wine ]; then
    export WINE=$LUTRIS_WINEFOLDERS/$LUTRIS_WINEVER/bin/wine
    export PATH="$LUTRIS_WINEFOLDERS/$LUTRIS_WINEVER/bin:$PATH"
    export WINEPREFIX=$HOME/.wine
    export WINEDLLOVERRIDES="d3d10core,d3d11,d3d9,dxgi=n;winemenubuilder.exe=d"
    export WINEESYNC=1
    export WINEDEBUG=-all
    export LUTRIS_WINE_SOURCED=1
fi
